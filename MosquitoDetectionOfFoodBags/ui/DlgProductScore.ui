    padding-left: 8px;
}

/* 数据展示标签 */
QLabel.data {
    color: #2F4F4F;          /* 深石板灰 */
    font-family: monospace;
    border: 1px solid #E0E0E0;
    background-color: #FFFFFF;
    padding: 6px 12px;
    border-radius: 4px;
}

/* 禁用状态 */
QLabel:disabled {
    color: #AAAAAA;
    background-color: transparent;
}

