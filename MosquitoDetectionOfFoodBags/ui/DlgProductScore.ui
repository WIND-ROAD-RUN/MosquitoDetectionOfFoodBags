}

/* 未选中状态 */
QRadioButton::indicator {
    width: 20px;
    height: 20px;
    border-radius: 10px;
    border: 2px solid #999;
    background: #ffffff;
}

/* 悬停效果 */
QRadioButton::indicator:hover {
    border: 2px solid #666;
}

/* 选中状态 */
QRadioButton::indicator:checked {
    border: 2px solid #FF6B00;  /* 橙色边框 */
}

/* 选中时的内圆 */
QRadioButton::indicator:checked {
    background-color: qradialgradient(
        cx:0.5, cy:0.5,
        radius:0.4,
        fx:0.5, fy:0.5,
        stop:0 #FF6B00,  /* 主橙色 */
        stop:1 transparent
    );
}

/* 禁用状态 */
QRadioButton:::disabled {
    border: 2px solid #ccc;
    background: #eee;
}

/* 点击动画效果 */
QRadioButton::indicator:pressed {
    border: 2px solid #CC5500;  /* 深橙色边框 */
    background-color: qradialgradient(
        cx:0.5, cy:0.5,
        radius:0.5,
        fx:0.5, fy:0.5,
        stop:0 #FFD8B3,  /* 浅橙色渐变 */
        stop:1 transparent
    );
}</string>
